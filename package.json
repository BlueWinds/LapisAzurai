{
  "name": "Lapis-Azurai",
  "description": "Adult business management / exploration game",
  "version": "2.0.1",
  "author": "BlueWinds",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "babel-preset-env": "^1.7.0",
    "coffee-loader": "^0.9.0",
    "coffeelint": "^2.1.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "happypack": "^5.0.1",
    "http-server": "^0.11.1",
    "nodemon": "^1.18.10",
    "npm-run-all": "^4.1.5",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-livereload-plugin": "^2.3.0"
  },
  "keywords": [
    "lapis",
    "game"
  ],
  "license": "GPL-3.0+",
  "readmeFilename": "./README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com:BlueWinds/LapisAzurai.git"
  },
  "scripts": {
    "start": "coffeelint -q src/ || true && run-s clean && run-p webpack:watch serve",
    "build-dist": "run-s compile remove-livereload && cd dist && zip -r LapisAzurai.zip *",
    "compile": "coffeelint -q src/ && run-s clean && webpack --mode production",
    "clean": "rm -rf dist/* || true",
    "remove-livereload": "sed -i 's/<script src=\"http:\\/\\/localhost:35729\\/livereload.js\"><\\/script>//' dist/index.html",
    "serve": "sleep 3 && http-server dist -os",
    "webpack:watch": "webpack --watch",
    "word-diff-add": "git diff --word-diff=porcelain | grep -e '^+[^+]' | wc -w",
    "word-diff-rm": "git diff --word-diff=porcelain | grep -e '^-[^-]' | wc -w",
    "word-diff": "echo $((`npm run word-diff-add` - `npm run word-diff-rm`))"
  },
  "coffeelintConfig": {
    "arrow_spacing": {
      "level": "ignore"
    },
    "braces_spacing": {
      "level": "error",
      "spaces": 0,
      "empty_object_spaces": 0
    },
    "camel_case_classes": {
      "level": "error"
    },
    "coffeescript_error": {
      "level": "error"
    },
    "colon_assignment_spacing": {
      "level": "error",
      "spacing": {
        "left": 0,
        "right": 1
      }
    },
    "cyclomatic_complexity": {
      "level": "error",
      "value": 10
    },
    "duplicate_key": {
      "level": "error"
    },
    "empty_constructor_needs_parens": {
      "level": "ignore"
    },
    "ensure_comprehensions": {
      "level": "error"
    },
    "eol_last": {
      "level": "error"
    },
    "indentation": {
      "value": 2,
      "level": "error"
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    },
    "missing_fat_arrows": {
      "level": "ignore"
    },
    "newlines_after_classes": {
      "value": 3,
      "level": "ignore"
    },
    "no_backticks": {
      "level": "error"
    },
    "no_debugger": {
      "level": "error",
      "console": false
    },
    "no_empty_functions": {
      "level": "error"
    },
    "no_empty_param_list": {
      "level": "error"
    },
    "no_implicit_braces": {
      "level": "ignore"
    },
    "no_implicit_parens": {
      "level": "ignore"
    },
    "no_interpolation_in_single_quotes": {
      "level": "error"
    },
    "no_nested_string_interpolation": {
      "level": "error"
    },
    "no_plusplus": {
      "level": "ignore"
    },
    "no_private_function_fat_arrows": {
      "level": "error"
    },
    "no_stand_alone_at": {
      "level": "ignore"
    },
    "no_tabs": {
      "level": "error"
    },
    "no_this": {
      "level": "error"
    },
    "no_throwing_strings": {
      "level": "error"
    },
    "no_trailing_semicolons": {
      "level": "error"
    },
    "no_trailing_whitespace": {
      "level": "error",
      "allowed_in_comments": false,
      "allowed_in_empty_lines": false
    },
    "no_unnecessary_double_quotes": {
      "level": "error"
    },
    "no_unnecessary_fat_arrows": {
      "level": "error"
    },
    "non_empty_constructor_needs_parens": {
      "level": "error"
    },
    "prefer_english_operator": {
      "level": "error",
      "doubleNotLevel": "ignore"
    },
    "space_operators": {
      "level": "error"
    },
    "spacing_after_comma": {
      "level": "error"
    },
    "transform_messes_up_line_numbers": {
      "level": "error"
    }
  },
  "dependencies": {
    "d3": "^3.5.17",
    "jquery": "^3.4.1",
    "jquery.scrollto": "^2.1.2",
    "js-yaml": "^3.13.1",
    "speed-measure-webpack-plugin": "^1.3.3"
  }
}
